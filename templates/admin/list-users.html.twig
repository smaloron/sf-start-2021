{% extends 'base-layout.html.twig' %}

{% block body %}
    <h1>Liste des utilisateurs</h1>

    <table class="table">
        <tr>
            <th>id</th>
            <th>Nom</th>
            <th>Roles</th>
        </tr>

        {% for user in userList %}
            <tr>
                <td>{{ user.id }}</td>
                <td> {{ user.userName }}</td>
                <td>
                    {% for role in user.roles %}
                        {{ role }}
                    {% endfor %}
                </td>
            </tr>
        {% endfor %}
    </table>

{% endblock %}


{% block title %}

{% endblock %}

